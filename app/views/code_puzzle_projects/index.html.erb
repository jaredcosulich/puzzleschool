<p id="notice"><%= notice %></p>

<h1>Code Puzzle Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code puzzle class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_puzzle_projects.each do |code_puzzle_project| %>
      <tr>
        <td><%= code_puzzle_project.name %></td>
        <td><%= code_puzzle_project.code_puzzle_class_id %></td>
        <td><%= link_to 'Show', code_puzzle_project %></td>
        <td><%= link_to 'Edit', edit_code_puzzle_project_path(code_puzzle_project) %></td>
        <td><%= link_to 'Destroy', code_puzzle_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code Puzzle Project', new_code_puzzle_project_path %>
