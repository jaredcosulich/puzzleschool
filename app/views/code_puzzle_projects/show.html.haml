- @noquotesandsubscriber = true

.row.text-xs-center.py-3.mt-3
  .col-xs-12
    .canvas
      %canvas#drawing_canvas
      %canvas#arrow_canvas

    .signature-container
      .signature
      .signature-color
      
    .cards-container
      .cards
        - @code_puzzle_project.code_puzzle_cards.each do |card|
          .card{id: "card_#{card.id}", data: {id: card.id, code: card.code, param: card.param, photo: card.photo_url}}
            .image= image_tag(card.photo_url)
